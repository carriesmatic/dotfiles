# Java for Minecraft
 alias java_ls='/usr/libexec/java_home -V 2>&1 | grep -E "\d.\d.\d_\d\d" | cut -d , -f 1 | colrm 1 4 | grep -v Home'

 function java_use() {
     export JAVA_HOME=$(/usr/libexec/java_home -v $1)
     export PATH=$JAVA_HOME/bin:$PATH
     java -version
}	

# Kindle
alias kindleup="sudo ifconfig en6 up;sudo ifconfig en6 192.168.15.201"
alias kindledown="sudo ifconfig en6 up;sudo ifconfig en6 192.168.15.201"
alias kindle="ssh root@192.168.15.244"